n  }, [data]);\r\n\r\n  // mutation\r\n  const [userUpdate] = useMutation(USER_UPDATE, {\r\n    // data all the values returned\r\n    update: ({ data }) => {\r\n      console.log(\"USER UPDATE MUTATION IN PROFILE\", data);\r\n      toast(`Profile updated`, {\r\n        position: toast.POSITION.BOTTOM_CENTER,\r\n        autoClose: 3000,\r\n        draggablePercent: 60,\r\n      });\r\n      setLoading(false);\r\n    },\r\n  });\r\n\r\n  const handleSubmit = (e) => {\r\n    e.preventDefault();\r\n    setLoading(true);\r\n\r\n    // execute mutation\r\n    userUpdate({ variables: { input: values } });\r\n  };\r\n  const handleChange = (e) => {\r\n    setValues({ ...values, [e.target.name]: e.target.value });\r\n  };\r\n\r\n  return (\r\n    <div className=\"container p-4 profile\">\r\n      <FileUpload\r\n        setValues={setValues}\r\n        setLoading={setLoading}\r\n        values={values}\r\n      />\r\n\r\n      <UserProfile\r\n        handleSubmit={handleSubmit}\r\n        handleChange={handleChange}\r\n        loading={loading}\r\n        values={values}\r\n      />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Profile;\r\n","C:\\Users\\user\\Desktop\\Web\\articleGram\\client\\src\\pages\\post\\Post.js",[],"C:\\Users\\user\\Desktop\\Web\\articleGram\\client\\src\\pages\\auth\\PasswordForgot.js",[],"C:\\Users\\user\\Desktop\\Web\\articleGram\\client\\src\\components\\graphql\\queries.js",[],"C:\\Users\\user\\Desktop\\Web\\articleGram\\client\\src\\components\\graphql\\fragments.js",[],"C:\\Users\\user\\Desktop\\Web\\articleGram\\client\\src\\components\\graphql\\mutations.js",[],"C:\\Users\\user\\Desktop\\Web\\articleGram\\client\\src\\components\\forms\\UserProfile.js",[],"C:\\Users\\user\\Desktop\\Web\\articleGram\\client\\src\\components\\FileUpload.js",[],"C:\\Users\\user\\Desktop\\Web\\articleGram\\client\\src\\components\\Image.js",[],"C:\\Users\\user\\Desktop\\Web\\articleGram\\client\\src\\components\\LoadingToRedirect.js",[],"C:\\Users\\user\\Des